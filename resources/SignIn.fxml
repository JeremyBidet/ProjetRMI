<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.TilePane?>
<?import javafx.scene.layout.VBox?>

<VBox fx:controller="SignInController" prefHeight="172.0" prefWidth="314.0" xmlns="http://javafx.com/javafx/8.0.65" xmlns:fx="http://javafx.com/fxml/1">
  <children>
      <TilePane VBox.vgrow="ALWAYS">
         <children>
          <AnchorPane accessibleText="login" maxHeight="-1.0" maxWidth="-1.0" prefHeight="169.0" prefWidth="314.0">
            <children>
                  <TextField fx:id="login" layoutX="120.0" layoutY="28.0" />
                  <PasswordField fx:id="password" layoutX="120.0" layoutY="68.0" />
                  <Label layoutX="42.0" layoutY="33.0" text="Login :" />
                  <Label layoutX="29.0" layoutY="73.0" text="Password :" />
                  <Button fx:id="signIn" layoutX="71.0" layoutY="112.0" mnemonicParsing="false" text="Sign In" onAction="#loginButtonHandler"/>
                  <Button fx:id="signUp" layoutX="204.0" layoutY="112.0" mnemonicParsing="false" text="Sign Up" onAction="#registerButtonHandler"/>
            </children>
          </AnchorPane>
         </children>
      </TilePane>
  </children>
</VBox>
